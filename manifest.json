{
    "manifest_version": 3,
    "name": "Déclaration TVA",
    "version": "1.0",
    "description": "extention Déclaration TVA MRP",
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
      },
    "host_permissions": [
        "http://192.168.100.3:5000/*"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["*://tva.tax.gov.ma/*"],
            "js": ["content.js"]
        }
    ],"icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
}
